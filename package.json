  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmmirand/clean-workflows-logs-runs/issues"
  },
  "homepage": "https://github.com/jmmirand/clean-workflows-logs-runs#readme",
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.0.0",
    "jest": "^27.2.5"
  }
}
